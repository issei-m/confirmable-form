{
    "name": "issei-m/confirmable-form",
    "description": "Provides the `OperationType` form type needed to implement confirmable form (i.e. two-step form), and extension to install it to your project's form.",
    "type": "library",
    "keywords": ["form", "confirmable", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Issei Murasawa",
            "email": "issei.m7@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Issei\\ConfirmableForm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Issei\\ConfirmableForm\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4",
        "symfony/form": "~2.7|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
